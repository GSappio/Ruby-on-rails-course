<!DOCTYPE html>
<html>
  <head>
    <title>CrypoWallet</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>


    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'scaffolds', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag params[:controller], media: 'all', 'data-turbolinks-track': 'reload' %>


    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>


    <%= stylesheet_link_tag params[:controller], 'data-turbolinks-track': 'reload' %>

    
  </head>

  <body>
  <h1>HEADER - <%= params[:controller] %></h1>
    <%= yield %>
    <h1>FOOTER</h1>
  </body>
</html>
